    {
      "description": "Swap Ctrl/Alt + arrows" ,
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["right_command", "control"]
            }
          },
          "to": {
            "key_code": "left_arrow",
            "modifiers": [
              "option"
            ]
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["right_command", "control"]
            }
          },
          "to": {
            "key_code": "right_arrow",
            "modifiers": [
              "option"
            ]
          }
        }
      ]
    },
    {
      "description": "Test",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {"mandatory": ["left_control"]}
          },
          "to": [ {"key_code": "a"} ]
        }
      ]
    },
    {
      "description": "Double tap Ctrl + [ to Esc",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [
            {
              "key_code": "escape"
            },
            {
              "set_variable": {
                "name": "esc_shortcut_mode",
                "value": 0
              }
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "esc_shortcut_mode",
              "value": 1
            },
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.googlecode\\.iterm2"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "esc_shortcut_mode",
                "value": 1
              }
            },
            {
              "key_code": "open_bracket",
              "modifiers": [
                "left_control"
              ]
            }
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              {
                "set_variable": {
                  "name": "esc_shortcut_mode",
                  "value": 0
                }
              }
            ],
            "to_if_canceled": [
              {
                "set_variable": {
                  "name": "esc_shortcut_mode",
                  "value": 0
                }
              }
            ]
          },
          "parameters": {
            "basic.to_delayed_action_delay_milliseconds": 200
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "esc_shortcut_mode",
              "value": 0
            },
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.googlecode\\.iterm2"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Additional emacs key bindings [control+m/u]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
